<div id="barra_superior"><center>Bienvenido: <%= @user.name %></center></div>
<h1>Sube una Selfie!</h1>

<form method="post" enctype='multipart/form-data' action="/upload">
  <br><br>
  <input type='file' name='photo[photo]'>
  <br><br>
  <br/>
  <input class="boton" type='submit' value='Upload!'>
</form>

<a href="/end_session"><button class="boton">Cerrar Sesion</button></a>

<div class="carousel">
  <ul class="frames">
    <% @photos.each do |photo| %>
    <li>
      <img src="<%= photo.photo_url(:thumb) %>">
    </li>
    <% end %>
  </ul>
</div>